              "gx_accent": {
                "h": 100,
                "s": 100,
                "l": 60
              },
              "gx_secondary_base": {
                "h": 185,
                "s": 60,
                "l": 16
              }
            },
            "light": {
              "gx_accent": {
                "h": 224,
                "s": 90,
                "l": 50
              },
              "gx_secondary_base": {
                "h": 222,
                "s": 81,
                "l": 87
              }
            }
          }
        ],
        "wallpaper": [
          {
            "id": "wallpaper_01",
            "name": "Stellar Blade (Static Wallpaper)",
            "dark": {
              "image": "wallpaper/static_dark.jpg",
               "image_mobile": "wallpaper/static_dark_mobile.jpg",
              "speeddial_position": "bottom",
              "text_color": "#77ff33",
              "text_shadow": "#FFFFFF",
              "ui_settings": {
                "background_blur": 30,
                "background_opacity": 75,
                "focus_mode": false,
                "islands_opacity": 85,
                "vignette_strength": 25
              }
            },
            "light": {
              "image": "wallpaper/static_light.jpg",
               "image_mobile": "wallpaper/static_light_mobile.jpg",
              "speeddial_position": "top",
              "text_color": "#0D4AF2",
              "text_shadow": "#FFFFFF",
              "ui_settings": {
                "background_blur": 10,
                "background_opacity": 30,
                "focus_mode": false,
                "islands_opacity": 70,
                "vignette_strength": 20
              }
            }
          }
