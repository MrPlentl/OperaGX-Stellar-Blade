              "music/stellar-blade-chill-trax.mp3"
            ]
          }
        ],
        "browser_sounds": [
          {
            "id": "browser_sounds_01",
            "name": "Stellar Blade GX (Browser Sounds)",
            "sounds": {
              "CLICK": [
                "sounds/sb_click.mp3"
              ],
              "FEATURE_SWITCH_OFF": [
                "sounds/sb_feature_switch_off.mp3"
              ],
              "FEATURE_SWITCH_ON": [
                "sounds/sb_feature_switch_on.mp3"
              ],
              "HOVER_UP": [
                "sounds/sb_hover_up.mp3"
              ],
              "HOVER": [
                "sounds/sb_hover.mp3"
              ],
              "IMPORTANT_CLICK": [
                "sounds/sb_important_click.mp3"
              ],
              "LEVEL_UPGRADE": [
                "sounds/sb_level_upgrade.mp3"
              ],
              "LIMITER_OFF": [
                "sounds/sb_limiter_off.mp3"
              ],
              "LIMITER_ON": [
                "sounds/sb_limiter_on.mp3"
              ],
              "SWITCH_TOGGLE": [
                "sounds/sb_switch_toggle.mp3"
