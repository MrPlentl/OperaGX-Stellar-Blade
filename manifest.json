        "page_styles": [
          {
            "id": "page_styles_01",
            "name": "Stellar Blade GX (page_styles)",
            "css": [
              "webmodding/youtube.css"
            ],
            "matches": [
              "https://*.youtube.com/*"
            ]
          }
        ],
        "theme": [
          {
            "id": "theme_01",
            "name": "Stellar Blade GX (Theme)",
