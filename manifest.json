{
    "name": "Stellar Blade GX",
    "description": "Reclaim Earth for Humankind by using this Opera GX mod. Eve needs your help!",
    "update_url": "https://api.gx.me/store/mods/update",
    "version": "0.6",
    "developer": {
      "name": "ThaBamboozler"
    },
    "manifest_version": 3,
    "icons": {
      "512": "icon_512.png"
    },
    "mod": {
      "license": "license.txt",
      "schema_version": 2,
      "tags": [ "stellar blade", "stellarblade", "stellar-blade", "stelar blade", "steler blade" ],
      "payload": {
        "background_music": [
          {
            "id": "background_music_01",
            "name": "Stellar Blade GX (BG Music Chill Trax)",
